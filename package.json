{
  "name": "electron-react-hot-ts-boilerplate",
  "version": "1.0.0",
  "description": "Electron React App with Typescript and Hot Reload",
  "keywords": [
    "electron",
    "react",
    "hot-reload",
    "hmr",
    "typescript"
  ],
  "author": "thavixt",
  "homepage": "",
  "license": "MIT",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron ./dist/main.js",
    "devstart": "cross-env ELECTRON_DEV=dev electron ./dist/main.js",
    "dev": "webpack --config ./webpack.config.dev.js",
    "build": "webpack --config ./webpack.config.prod.js",
    "server": "npm run dev && webpack-dev-server --config ./webpack.config.server.js",
    "test": "echo \"Error: no tests specified\" && exit 1"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-hot-loader": "^4.1.1",
    "@types/webpack-env": "^1.15.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^15.5.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.0",
    "react-hot-loader": "^4.12.21",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
